<div id="sessions">
  <% @sessions.each do |session| %>
    <p>
      <strong>
        <%= link_to session.sport + " in " + session.spot.name, session %>
      </strong><br>
      Surfer: <%= session.user.name %><br>
      Visibility: <%= session.visibility %><br>
      When: <%= time_tag session.when %><br>
      Tracks: <%= session.tracks.size %>
    </p>
  <% end %>
  <%= paginate @sessions %>
</div>